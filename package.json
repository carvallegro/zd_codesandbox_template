    "@zendeskgarden/react-theming": "^8.31.0",
    "@zendeskgarden/react-typography": "^8.31.0",
    "@zendeskgarden/svg-icons": "^6.10.0",
    "date-fns": "^2.16.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "formik": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
